{
    "name": "yeaha/owl-core",
    "description": "Owl Framework Core Component",
    "homepage": "https://github.com/yeaha/owl-core",
    "license": "MIT",
    "authors": [
        {
            "name": "YangYi",
            "email": "yangyi.cn.gz@gmail.com",
            "homepage": "https://github.com/yeaha"
        }
    ],
    "require": {
        "php": ">=8.0.0",
        "psr/log": "~1.0",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "Owl\\Middleware\\": "src/Middleware/",
            "Owl\\Parameter\\": "src/Parameter/",
            "Owl\\Traits\\": "src/Traits/",
            "Owl\\": "src/"
        },
        "files": [
            "src/constant.php",
            "src/functions.php",
            "src/init.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
